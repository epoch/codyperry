<!DOCTYPE html>
<html>
<head>
  <title>DishApi</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <button id="magic-btn">Add dish</button>
  <div id="container">
  </div>

  <script id="dish-item-template" type="text/x-underscore-template">
    <div class="dish-item">
      <img src="http://placehold.it/400x400" alt="">
      <div class="content">
        <h2><a href="#">{{ title }}</a></h2>
        <h4>at general assembly</h4>
        <p><span class="count">{{ starCount }}</span> stars</p>
        <span class="star">&#9733;</span>
      </div>
    </div>  
  </script>

  <script></script>

</body>
</html>
